{
  "name": "towers",
  "version": "0.1.0",
  "repository": "git@github.com:Chelny/towers.git",
  "author": {
    "name": "Chelny",
    "url": "https://www.linkedin.com/in/chelny/"
  },
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "prepare": "husky",
    "postinstall": "prisma generate --no-engine",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "vitest",
    "lint:scss": "npx stylelint '**/*.scss'",
    "lint:ts": "npx eslint '**/*.{ts,tsx}'",
    "lint": "npm run lint:scss && npm run lint:ts",
    "lint:fix": "npm run lint:scss -- --fix && npm run lint:ts -- --fix",
    "prisma:format": "npx prisma format",
    "prisma:generate": "npx prisma generate --no-engine",
    "prisma:push": "npx prisma db push",
    "prisma:migrate:dev": "npx prisma migrate dev --name init",
    "prisma:migrate:prod": "npx prisma migrate deploy",
    "prisma:seed:dev": "npx prisma db seed -- --environment development",
    "prisma:seed:prod": "npx prisma db seed -- --environment production",
    "prisma:studio": "npx prisma studio --port 5555 --browser none",
    "prisma:reset": "npx prisma db push --force-reset"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.7.4",
    "@next/env": "15.0.3",
    "@prisma/client": "5.22.0",
    "@prisma/extension-accelerate": "1.2.1",
    "@reduxjs/toolkit": "2.3.0",
    "@simplewebauthn/browser": "11.0.0",
    "@simplewebauthn/server": "11.0.0",
    "@sinclair/typebox": "0.34.8",
    "@types/react": "npm:types-react@rc",
    "@vercel/functions": "1.5.0",
    "axios": "1.7.7",
    "bcrypt-edge": "0.1.0",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "isomorphic-dompurify": "2.16.0",
    "next": "15.0.3",
    "next-auth": "5.0.0-beta.25",
    "node-cron": "3.0.3",
    "react": "19.0.0-rc.1",
    "react-dom": "19.0.0-rc.1",
    "react-icons": "5.3.0",
    "react-redux": "9.1.2",
    "redux": "5.0.1",
    "resend": "4.0.1",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.15.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.0.1",
    "@types/eslint__eslintrc": "2.1.2",
    "@types/node": "22.9.3",
    "@types/node-cron": "3.0.11",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@vitejs/plugin-react": "4.3.3",
    "eslint": "9.15.0",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-import": "2.31.0",
    "husky": "9.1.7",
    "jsdom": "25.0.1",
    "lint-staged": "15.2.10",
    "nodemon": "3.1.7",
    "postcss": "8.4.49",
    "prettier": "3.3.3",
    "prisma": "5.22.0",
    "sass": "1.81.0",
    "sass-loader": "16.0.4",
    "stylelint": "16.6.1",
    "stylelint-config-property-sort-order-smacss": "10.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-order": "6.0.4",
    "tailwindcss": "3.4.15",
    "ts-node": "10.9.2",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "vitest": "2.1.5"
  }
}
