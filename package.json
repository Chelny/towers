{
  "name": "towers",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Chelny",
    "url": "https://www.linkedin.com/in/chelny/"
  },
  "type": "module",
  "scripts": {
    "dev": "nodemon server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "vitest --silent",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "lint:scss": "npx stylelint '**/*.scss'",
    "lint:ts": "npx eslint '**/*.{ts,tsx}'",
    "lint": "npm run lint:scss && npm run lint:ts",
    "lint:fix": "npm run lint:scss -- --fix && npm run lint:ts -- --fix",
    "prisma:generate:dev": "npx prisma format && npx prisma generate",
    "prisma:push:dev": "npx prisma db push",
    "prisma:seed:dev": "npx prisma db seed -- --environment development",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:studio:dev": "npx prisma studio",
    "prisma:reset:dev": "npx prisma db push --force-reset"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.4.2",
    "@prisma/client": "5.18.0",
    "@reduxjs/toolkit": "2.2.7",
    "@simplewebauthn/browser": "9.0.1",
    "@simplewebauthn/server": "9.0.3",
    "@sinclair/typebox": "0.32.34",
    "@testing-library/react-hooks": "8.0.1",
    "axios": "1.7.5",
    "bcryptjs": "2.4.3",
    "clsx": "2.1.1",
    "date-fns": "3.6.0",
    "isomorphic-dompurify": "2.15.0",
    "next": "14.2.3",
    "next-auth": "5.0.0-beta.20",
    "node-cron": "3.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.2.1",
    "react-redux": "9.1.2",
    "redux": "5.0.1",
    "resend": "3.5.0",
    "socket.io": "4.7.5",
    "socket.io-client": "4.7.5",
    "uuid": "10.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "16.0.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "22.1.0",
    "@types/node-cron": "3.0.11",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/socket.io": "3.0.2",
    "@types/socket.io-client": "3.0.0",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.7.0",
    "@vitejs/plugin-react": "4.3.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-plugin-import": "2.29.1",
    "husky": "9.0.11",
    "jsdom": "25.0.0",
    "lint-staged": "15.2.7",
    "nodemon": "3.1.4",
    "postcss": "^8",
    "prettier": "3.3.2",
    "prisma": "5.18.0",
    "sass": "1.77.6",
    "stylelint": "16.6.1",
    "stylelint-config-property-sort-order-smacss": "10.0.0",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-order": "6.0.4",
    "tailwindcss": "^3.4.1",
    "ts-node": "10.9.2",
    "tsx": "4.16.5",
    "typescript": "5.4.5",
    "vitest": "2.0.5"
  }
}
