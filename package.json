    "@reduxjs/toolkit": "2.3.0",
    "@sinclair/typebox": "0.34.8",
    "@socket.io/redis-adapter": "8.3.0",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.0",
    "axios": "1.7.7",
    "bcrypt-edge": "0.1.0",
    "better-auth": "1.1.10",
    "better-status-codes": "2.0.7",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "immer": "10.1.1",
