{
  "name": "towers",
  "version": "0.1.0",
  "repository": "git@github.com:Chelny/towers.git",
  "author": {
    "name": "Chelny Duplan",
    "url": "https://www.linkedin.com/in/chelny/"
  },
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon",
    "prepare": "husky",
    "postinstall": "pnpm prisma:generate",
    "build": "next build",
    "start": "NODE_ENV=production tsx server.ts",
    "test": "vitest",
    "translations:extract": "lingui extract --clean",
    "translations:compile": "lingui compile --typescript",
    "lint:scss": "stylelint '**/*.scss'",
    "lint:ts": "eslint '**/*.{ts,tsx}'",
    "lint": "pnpm lint:scss && pnpm lint:ts",
    "lint:fix": "pnpm lint:scss --fix && pnpm lint:ts --fix",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:seed:dev": "BYPASS_LINGUI_ERRORS=true prisma db seed -- --environment development",
    "prisma:seed:prod": "BYPASS_LINGUI_ERRORS=true prisma db seed -- --environment production",
    "prisma:studio": "prisma studio --port 5555 --browser none",
    "prisma:reset": "prisma db push --force-reset",
    "better-auth:generate": "BYPASS_LINGUI_ERRORS=true npx @better-auth/cli generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.7.4",
    "@better-fetch/fetch": "1.1.12",
    "@lingui/core": "5.1.2",
    "@lingui/react": "5.1.2",
    "@next/third-parties": "15.1.2",
    "@paralleldrive/cuid2": "2.2.2",
    "@prisma/client": "6.0.1",
