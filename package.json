    "pino": "9.6.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "5.3.0",
    "react-redux": "9.1.2",
    "redux": "5.0.1",
    "resend": "4.0.1",
    "socket.io": "4.8.1",
    "socket.io-client": "4.8.1",
    "zustand": "5.0.3"
  },
  "devDependencies": {
